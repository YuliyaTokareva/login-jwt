{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "baseUrl": "./src",
    "incremental": true,
    "paths": {
      //"@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"] // this mapping is relative to "baseUrl"
      // "@components/*": ["src/multisite/components/*"],
      // "@common/*": ["src/common/*"],
      // "@entities/*": ["src/entities/*"],
      "@img/*": ["img/*"]
      // "@theme/*": ["src/theme/*"],
      // "@theme": ["src/theme"]
    }
  },
  "include": ["src", "index.d.ts"]
}
